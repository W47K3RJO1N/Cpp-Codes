#include <bits/stdc++.h>
using namespace std;

void solidsquare() {
    for (int i = 1; i <= 4; i++) {
        for (int j = 1; j <= 4; j++) {
            cout << "* ";
        }
        cout << endl;
    }
}

int main() {
    solidsquare();
    return 0;
}